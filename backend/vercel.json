{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/uploads/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/src/app.js"
    }
  ]
}
